{
  "Logging": {
    "Console": { // override the logging for console
      "Loglevel": {
        "Default": "Trace",
        "Microsoft*": "Warning"
      }
    },
    "Debug": { // override the logging for Debug
      "Loglevel": {
        "Default": "Warning",
        "Microsoft*": "Warning"
      }
    },
    "LogLevel": { // default
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
    }
  },
  "AllowedHosts": "*"
}
